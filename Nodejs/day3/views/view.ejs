</head>
<body>
<table class="table">
        <th>index</th>
        <th>name</th>
        <th>salary</th>
          <%for(var i=0;i<empdata.length;i++)
          {%>
             <tr>
                 <td><%= i+1 %> </td>
                 <td> <%= empdata[i].Fname %></td>
                 <td> <%= empdata[i].salary %></td>
             </tr> 
          <%}%>
</table>

</body>
